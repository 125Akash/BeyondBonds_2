{
  "scripts": {
    "clientdev": "cd client && npm install",
    "serverdev": "cd server && npm install",
    "devall": "concurrently \"npm run serverdev\" \"npm run clientdev\"",
    "client": "cd client && npm start",
    "server": "cd server && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "Fire": "npm install & concurrently \"npm run devall\" \"npm run dev\""
  },
  "dependencies": {
    "concurrently": "^8.2.1"
  }
}
